[version]
current = 0.1.2+build.92

[options]
auto-tag = True
auto-commit = True
auto-push = True
signature = True

[file1]
file = ./bumpster/__init__.py
regex = __version__\s*=\s*(?P<quote>[\'\"])(?P<version>.+)(?P=quote)
style = semantic

[file2]
file = ./bumpster/__init__.py
regex = __public_version__\s*=\s*(?P<quote>[\'\"])(?P<version>.+)(?P=quote)
style = public

