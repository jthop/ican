[version]
current = 0.1.6+build.106

[options]

[file: version]
file = ./ican/__init__.py
variable = __version__
style = semantic

[file: public]
file = ./ican/__init__.py
variable = __public_version__
style = public

[pipeline: release]
step1 = git add .
step2 = git commit -m "{{msg}}"
step3 = git tag -a {{tag}} --sign
step4 = git push origin master {{tag}}

