[version]
current = 0.1.4+build.97

[options]
auto-tag = True
auto-commit = True
auto-push = True
signature = True

[file1]
file = ./ican/__init__.py
regex = __version__\s*=\s*(?P<quote>[\'\"])(?P<version>.+)(?P=quote)
style = semantic

[file2]
file = ./ican/__init__.py
regex = __public_version__\s*=\s*(?P<quote>[\'\"])(?P<version>.+)(?P=quote)
style = public

